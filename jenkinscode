pipeline {
   agent any
      stages {
         stage('Hello') {
               steps {
               echo 'Hello World'
               sh 'git --version'
               }
            }
      stage('python Build') {
            steps {
                sh 'sudo docker images'
                //git branch: 'master', url: 'https://github.com/Gitdevops34/Testingcode.git'
                //sh 'sudo docker build -t my-python . -f dockerfile1'
                sh 'python3 python.py'
                echo "Hello are your there"
            }
        } 
		}
    }
