pipeline {
   agent any
      stages {
         stage('Hello') {
               steps {
               echo 'Hello World'
               sh 'git --version'
               }
            }
      stage('docker Build') {
            steps {
                //sh 'sudo docker images'
                //git branch: 'master', url: 'https://github.com/Gitdevops34/Testingcode.git'
                sh 'sudo docker build -t my-python . -f dockerfile1'
                sh 'sudo docker run my-python'
                //bat 'python ops.py'
            }
        } 
		}
    }
