pipeline {
   agent any
      stages {
         stage('Hello') {
               steps {
               echo 'Hello World'
               sh 'git --version'
               sh 'v'
               }
            }
      stage('Build') {
            steps {
                sudo docker images
                //git branch: 'master', url: 'https://github.com/Gitdevops34/Testingcode.git'
                docker build -t my-python . -f dockerfile1
                docker run my-python
                //bat 'python ops.py'
            }
        } 
		}
    }
